{
  "name": "saxophone",
  "description": "Fast and lightweight event-driven XML parser in pure JavaScript",
  "version": "0.6.1",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/matteodelabre/saxophone.git"
  },
  "scripts": {
    "lint": "eslint .",
    "tape": "tape 'lib/**/*.test.js'",
    "test": "npm run --silent tape | faucet",
    "coverage": "nyc --reporter=html --reporter=text npm --silent run tape",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "benchmark": "node benchmark"
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "lib/**/*.test.js"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "xml",
    "parser",
    "stream",
    "sax",
    "fast"
  ],
  "files": [
    "lib/**/*"
  ],
  "author": {
    "name": "Matt√©o Delabre",
    "email": "contact@matteodelabre.me",
    "url": "https://github.com/matteodelabre"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "common-tags": "^1.8.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.15.0",
    "faucet": "0.0.1",
    "lodash.uniq": "^4.5.0",
    "nyc": "^15.1.0",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1"
  },
  "dependencies": {
    "readable-stream": "^3.6.0",
    "string_decoder": "^1.3.0"
  }
}
